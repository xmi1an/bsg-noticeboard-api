{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/notices",
      "dest": "/app.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
